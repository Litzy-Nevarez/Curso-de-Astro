---
    interface Props{
        icon: string
        text: string
    }

    const { icon, text } = Astro.props

---

<div class="py-10 space-y-5 flex flex-col items-center">
    <img class="max-w-52" src={ icon } alt={`Icono `} />
    <h3 class="text-4xl text-coffee-800 font-secundary">{ text }</h3>
</div>